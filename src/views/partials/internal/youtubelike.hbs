<form method="POST" action="/youtubelikes/{{data.id}}">
	{{> internal/id id=data.id type="youtube" }}

	{{> internal/crudRow emoji="📺" name="title" value=data.title }}
	{{> internal/crudRow emoji="👤" name="channel" value=data.channel }}
	{{> internal/crudRow emoji="🔗" name="url" value=data.url }}
	{{> internal/crudRowNumber emoji="⏱" name="duration_secs" value=data.duration_secs }}
	{{> internal/crudRow emoji="🕗" name="created_at" value=data.created_at }}

	{{> internal/crudSubmit }}
</form>
