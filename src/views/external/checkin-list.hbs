<div class="panel margin">
	{{> panelHeader title="recent check-ins" url="/" }}
	<div class="content">
		{{#if checkins}}
			{{#each checkins as |checkin|}}
				{{> checkin data=checkin }}
			{{/each}}
		{{else}}
			<br/>
			<div class="error rounded-panel">
				<p class="lead">I've not checked in anywhere! ğŸ˜­</p>
			</div>
		{{/if}}
	</div>
</div>

{{> pagination pagination=pagination route="/checkin" }}
